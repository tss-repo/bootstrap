{
  "name": "zetta-code/tss-bootstrap",
  "type": "library",
  "description": "TSS Bootstrap",
  "license": "MIT",
  "keywords": [
    "zf2",
    "tss",
    "bootstrap"
  ],
  "homepage": "https://github.com/zetta-code/tss-bootstrap",
  "authors": [
    {
      "name": "Thiago S. Santos",
      "email": "thiagos.xsantos@gmail.com",
      "homepage": "http://thiagossantos.com.br",
      "role": "Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "TSS\\Bootstrap\\": "src/"
    }
  },
  "require": {
    "php": "^5.5 || ^7.0",
    "zendframework/zend-stdlib": "^3.0"
  },
  "require-dev": {
    "zendframework/zend-mvc": "^3.0",
    "zendframework/zend-form": "^2.9",
    "zendframework/zend-navigation": "^2.8",
    "zendframework/zend-paginator": "^2.7"
  },
  "extra": {
    "zf": {
      "component": "TSS\\Bootstrap",
      "config-provider": "TSS\\Bootstrap\\ConfigProvider"
    }
  }
}
